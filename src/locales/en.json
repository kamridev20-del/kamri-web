    "questions": {
      "where_is_order": {
        "question": "Where is my order?",
        "answer": "You can track your order in real time from your customer area. We also send you email notifications at each stage of shipping. If you can't find your order, contact our customer service."
      },
      "how_to_return": {
        "question": "How to return a product?",
        "answer": "Returns are simple and free! Log in to your account, select the order in question and click \"Return\". We will send you a prepaid return label. You have 30 days to make your return."
      },
      "delivery_times": {
        "question": "What are the delivery times?",
        "answer": "Standard delivery is 2-3 business days in mainland France. For express delivery, count 24 hours. International deliveries take 5-7 business days depending on the destination."
      },
      "payment_methods": {
        "question": "What payment methods do you accept?",
        "answer": "We accept all credit cards (Visa, Mastercard, American Express), PayPal, Apple Pay, Google Pay and bank transfers. All payments are secured by SSL encryption."
      },
      "modify_cancel_order": {
        "question": "Can I modify or cancel my order?",
        "answer": "You can modify or cancel your order within 2 hours of validation. After this time, the order is being prepared and can no longer be modified. Contact us in case of emergency."
      },
      "create_account": {
        "question": "How to create an account?",
        "answer": "Click on \"Login\" at the top of the page, then on \"Create an account\". Fill out the form with your email and a secure password. You will receive a confirmation email."
      },
      "reset_password": {
        "question": "How to reset my password?",
        "answer": "On the login page, click \"Forgot password\". Enter your email address and you will receive a link to reset your password."
      },
      "warranty": {
        "question": "Are products guaranteed?",
        "answer": "Yes, all our products come with a one-year warranty against manufacturing defects. If you have a problem, contact our customer service with your order number."
      },
      "international_delivery": {
        "question": "Do you offer international delivery?",
        "answer": "Yes, we deliver to many countries. Delivery times and fees vary by destination. Check our delivery page for more details."
      },
      "contact_support": {
        "question": "How to contact customer service?",
        "answer": "You can contact us via the contact form on this page, by email at support@kamri.com, or by phone at +33 1 23 45 67 89 (Mon-Fri, 9am-6pm)."
      },
      "return_policy": {
        "question": "What is your return policy?",
        "answer": "We accept returns within 30 days of receipt. Items must be in their original condition, unworn and with their tags. Returns are free for orders over $50."
      },
      "track_order": {
        "question": "How can I track my order?",
        "answer": "Once your order is shipped, you will receive an email with a tracking number. You can track your package in real time on our site or via the carrier."
      },
      "free_shipping": {
        "question": "Do you offer free shipping?",
        "answer": "Yes! Shipping is free for all orders over $50. For lower orders, standard shipping fees apply."
      },
      "defective_product": {
        "question": "What to do if my product is defective?",
        "answer": "If you receive a defective product, contact us immediately. We will send you a replacement product free of charge or proceed with a full refund according to your preference."
      }
    }
